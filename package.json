{
  "name": "AlumApp",
  "version": "1.0.0",
  "description": "Alum App - Website used by Morgan State Howard County Alumni Chapter",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'frontend/*'\" \"npm run frontend\"",
    "frontend": "cd frontend && npm run start",
    "install": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "heroku-postbuild": "npm run build",
    "cypress:open": "cypress open"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "cypress": "^4.4.0",
    "eslint-plugin-cypress": "^2.10.3",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "@phuocng/react-pdf-viewer": "^1.4.0",
    "@react-pdf/renderer": "^1.6.8",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.3.1",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "firebase-admin": "^8.10.0",
    "firebase-service": "^4.0.221",
    "http2": "^3.3.7",
    "if-env": "^1.0.4",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.16.105",
    "react-calendar": "^3.0.0",
    "react-google-login": "^5.1.1",
    "react-places-autocomplete": "^7.2.1",
    "react-slick": "^0.25.2",
    "recompose": "^0.30.0",
    "sequelize": "^5.21.5",
    "slick-carousel": "^1.8.1",
    "stripe": "^8.38.0"
  },
  "engines": {
    "node": "12.16.x"
  }
}
